package semana1;

public class _04_TablaMultiplicar {
	public static void main(String[] args) {
		// Generar la tabla de multiplicar del 1 al 10
		for (int i = 1; i <= 10; i++) {
			for (int j = 1; j <= 10; j++) {
				// Completa la multiplicación y la impresión del resultado
				int resultado = i * j;
				System.out.print(resultado + "\t"); // Usa tabulación para alinear las columnas
			}
			// Salto de línea al final de cada fila
			System.out.println();
		}
	}
}
